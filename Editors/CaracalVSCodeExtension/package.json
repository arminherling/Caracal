{
  "name": "caracal-vscode-extension",
  "displayName": "Caracal",
  "description": "Caracal language support for VS Code",
  "version": "2025.3.10",
  "publisher": "Armin Herling",
  "readme": "README.md",
  "author": {
    "name": "Armin Herling"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.98.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arminherling/Caracal.git"
  },
  "homepage": "https://github.com/arminherling/Caracal",
  "qna": "https://github.com/arminherling/Caracal/issues",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "caracal",
        "aliases": [
          "Caracal",
          ""
        ],
        "extensions": [
          ".cara"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "caracal",
        "scopeName": "source.caracal",
        "path": "./syntaxes/caracal.tmLanguage.json"
      }
    ]
  }
}